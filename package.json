  "scripts": {
    "app": "better-npm-run webpack-dev-server",
    "dev": "better-npm-run webpack-dashboard",
    "build:prod": "better-npm-run build:prod",
    "clean": "rimraf build/*",
    "flow": "flow check",
    "lint:js": "eslint --config .eslintrc src test",
    "lint:css": "echo \"Error: lint:css task not implemented\" && exit 1",
    "test": "karma start config/karma/karma.config.js",
    "test:dev": "better-npm-run test:dev",
    "test:bamboo": "better-npm-run test:bamboo"
  },
  "betterScripts": {
    "build:prod": {
      "command": "webpack --config config/webpack/webpack.config.babel.js",
      "env": {
        "BUILD": "production"
      }
    },
    "test:dev": {
      "command": "karma start config/karma/karma.config.js",
      "env": {
        "TEST": "dev"
      }
    },
    "test:bamboo": {
      "command": "karma start config/karma/karma.config.js",
      "env": {
        "TEST": "bamboo"
      }
    },
    "webpack-dashboard": {
      "command": "webpack-dashboard -- webpack-dev-server --config config/webpack/webpack.config.babel.js --progress --profile --colors",
      "env": {
        "BUILD": "development"
      }
    },
    "webpack-dev-server": {
      "command": "webpack-dev-server --config config/webpack/webpack.config.babel.js",
      "env": {
        "BUILD": "development"
      }
    }
  }
